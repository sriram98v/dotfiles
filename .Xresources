URxvt.scrollBar: false
URxvt.depth: 50
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: False

!Fonts

URxvt.font: xft:DejaVu Sans Mono for Powerline :pixelsize=16
URxvt.boldFont: xft:DejaVu Sans Mono for Powerline :pixelsize=16
URxvt.letterSpace: -1
urxvt.perl-ext-common: default,resize-font

Xft.dpi: 96
Xft.antialias: true
Xft.hintstyle: hintfull
Xft.hinting: true

*.internalBorder: 13

! special
*.foreground: #8aeff4
*.background: #282C34
*.cursorColor: #7ea2b4

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #fdf6e3

